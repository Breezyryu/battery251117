   "source": [
    "def load_battery_data(path, start_cycle=1, end_cycle=100, min_cap=0, cutoff=0.05, ini_rate=0.2):\n",
    "    \"\"\"\n",
    "    Main function to load battery data from a given path.\n",
    "    It automatically detects the cycler type (Toyo or PNE) and loads the data.\n",
    "    \"\"\"\n",
    "    try:\n",
    "        is_pne = check_cycler(path)\n",
    "    except ValueError as e:\n",
    "        print(e)\n",
    "        return None, None\n",
    "\n",
    "    print(f\"Detected Cycler Type: {'PNE' if is_pne else 'Toyo'}\")\n",
    "\n",
    "    if not is_pne:\n",
    "        print(\"Loading Toyo data...\")\n",
    "        cycle_df = load_toyo_cycle(path, min_cap=min_cap, ini_rate=ini_rate)\n",
    "        profile_df = load_toyo_profile(path, start_cycle, end_cycle, min_cap, cutoff, ini_rate)\n",
    "    else:\n",
    "        print(\"Loading PNE data...\")\n",
    "        cycle_df = load_pne_cycle(path, start_cycle, end_cycle, min_cap=min_cap, ini_rate=ini_rate)\n",
    "        profile_df = load_pne_profile(path, start_cycle, end_cycle)\n",
    "    \n",
    "    print(\"Data loading complete.\")\n",
    "    return cycle_df, profile_df"
   ]
  },
