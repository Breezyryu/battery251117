    "# 예제 2: PNE 데이터 로드\n",
    "pne_path = os.path.abspath(\"Rawdata/A1_MP1_4500mAh_T23_1/M02Ch073[073]\")\n",
    "print(f\"Attempting to load data from: {pne_path}\")\n",
    "if os.path.exists(pne_path):\n",
