   "source": [
    "### 단계 3: PNE 데이터 로드"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def pne_min_cap(raw_file_path, mincapacity, ini_crate):\n",
    "    if mincapacity == 0:\n",
    "        if \"mAh\" in raw_file_path:\n",
    "            try:\n",
    "                raw_file_path_re = re.sub(r'[._@\\$\\\\\$\\\\\$\\\\\$\\\\\$\\\\\(\)]', ' ', raw_file_path)\n",
    "                match = re.search(r'(\\d+([\\-.]\\d+)?)mAh', raw_file_path_re)\n",
    "                mincap = match.group(1).replace('-', '.')\n",
    "                return float(mincap)\n",
    "            except AttributeError:\n",
    "                pass # Fallback\n",
    "        restore_path = os.path.join(raw_file_path, \"Restore\")\n",
    "        if os.path.isdir(restore_path):\n",
    "            try:\n",
    "                first_file = os.path.join(restore_path, \"SaveData0001.csv\")\n",
    "                if os.path.exists(first_file) and os.stat(first_file).st_size != 0:\n",
    "                    inicapraw = pd.read_csv(first_file, sep=\",\", header=None, encoding=\"cp949\")\n",
    "                    if len(inicapraw) > 2:\n",
    "                        return int(round(abs(inicapraw.iloc[2, 9] / 1000)) / ini_crate)\n",
    "            except Exception as e:\n",
    "                print(f\"Could not determine capacity for PNE data: {e}\")\n",
    "                return 0\n",
    "    return mincapacity\n",
    "\n",
    "def load_pne_cycle(path, start_cycle, end_cycle, min_cap=0, ini_rate=0.2):\n",
    "    restore_path = os.path.join(path, \"Restore\")\n",
    "    if not os.path.isdir(restore_path):\n",
    "        print(f\"'Restore' directory not found in {path}\")\n",
    "        return pd.DataFrame()\n",
    "\n",
    "    # Use Dask to read all relevant files at once\n",
    "    files_pattern = os.path.join(restore_path, \"*SaveEndData*.csv\")\n",
    "    try:\n",
    "        raw_dd = dd.read_csv(files_pattern, header=None, engine='c', encoding='cp949', on_bad_lines='warn')\n",
    "    except Exception as e:\n",
    "        print(f\"Error reading PNE cycle data: {e}\")\n",
    "        return pd.DataFrame()\n",
    "\n",
    "    # Filter for relevant cycles\n",
    "    cycle_col = 27 # As per BatteryDataTool.py\n",
    "    filtered_dd = raw_dd[(raw_dd[cycle_col] >= start_cycle) & (raw_dd[cycle_col] <= end_cycle)]\n",
    "\n",
    "    # Select and rename columns for clarity\n",
    "    # Simplified version of pne_cycle_data\n",
    "    min_cap_val = pne_min_cap(path, min_cap, ini_rate)\n",
    "    \n",
    "    loop_data = filtered_dd[filtered_dd[2] == 8] # Condition for cycle summary\n",
    "    \n",
    "    # This is a simplified mapping. The original script has very complex logic.\n",
    "    result = loop_data[[27, 10, 11, 24]].compute()\n",
    "    result.columns = [\"TotlCycle\", \"Chg_Cap(uAh)\", \"Dchg_Cap(uAh)\", \"Temp(mC)\"]\n",
    "    \n",
    "    # Unit conversion\n",
    "    result[\"Chg_Cap(mAh)\"] = result[\"Chg_Cap(uAh)\"] / 1000\n",
    "    result[\"Dchg_Cap(mAh)\"] = result[\"Dchg_Cap(uAh)\"] / 1000\n",
    "    result[\"Temp(C)\"] = result[\"Temp(mC)\"] / 1000\n",
    "    if min_cap_val > 0:\n",
    "        result['Dchg_Ratio'] = result[\"Dchg_Cap(mAh)\"] / min_cap_val\n",
    "        result['Efficiency'] = result[\"Dchg_Cap(mAh)\"] / result[\"Chg_Cap(mAh)\"]\n",
    "\n",
    "    return result.drop(columns=[\"Chg_Cap(uAh)\", \"Dchg_Cap(uAh)\", \"Temp(mC)\"])\
",
    "\n",
    "def load_pne_profile(path, start_cycle, end_cycle):\n",
    "    restore_path = os.path.join(path, \"Restore\")\n",
    "    if not os.path.isdir(restore_path):\n",
    "        print(f\"'Restore' directory not found in {path}\")\n",
    "        return pd.DataFrame()\n",
    "\n",
    "    files_pattern = os.path.join(restore_path, \"*SaveData*.csv\")\n",
    "    try:\n",
    "        raw_dd = dd.read_csv(files_pattern, header=None, engine='c', encoding='cp949', on_bad_lines='warn')\n",
    "    except Exception as e:\n",
    "        print(f\"Error reading PNE profile data: {e}\")\n",
    "        return pd.DataFrame()\n",
    "\n",
    "    cycle_col = 27\n",
    "    filtered_dd = raw_dd[(raw_dd[cycle_col] >= start_cycle) & (raw_dd[cycle_col] <= end_cycle)]\n",
    "\n",
    "    # Simplified column selection and renaming\n",
    "    # 17:StepTime, 8:Voltage, 9:Current, 21:Temp1, 2:StepType\n",
    "    profile_dd = filtered_dd[[cycle_col, 17, 8, 9, 21, 2]]\n",
    "    \n",
    "    # Compute and process with pandas\n",
    "    profile_df = profile_dd.compute()\n",
    "    profile_df.columns = [\"Cycle\", \"StepTime(/100s)\", \"Voltage(uV)\", \"Current(uA)\", \"Temp(mC)\", \"StepType\"]\n",
    "\n",
    "    # Unit conversion\n",
    "    profile_df[\"TimeMin\"] = profile_df[\"StepTime(/100s)\"] / 100 / 60\n",
    "    profile_df[\"Voltage(V)\"] = profile_df[\"Voltage(uV)\"] / 1000000\n",
    "    profile_df[\"Current(A)\"] = profile_df[\"Current(uA)\"] / 1000000\n",
    "    profile_df[\"Temp(C)\"] = profile_df[\"Temp(mC)\"] / 1000\n",
    "\n",
    "    return profile_df[[\"Cycle\", \"TimeMin\", \"Voltage(V)\", \"Current(A)\", \"Temp(C)\", \"StepType\"]]
   ]
  },
