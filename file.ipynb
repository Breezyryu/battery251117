    "pne_path = r\"C:\\Users\\Ryu\\Python_project\\data\\battery251027\\Rawdata\\A1_MP1_4500mAh_T23_1\"\n",
    "pne_cycle, pne_profile = load_battery_data(pne_path, profile_cycle_to_load=1)\n",
